points_scored <- c(15, 22, 25, 27, 30, 32, 35, 37, 40, 42, 45, 47, 50, 52, 55, 57, 60, 62, 65, 67, 70, 72, 75, 77, 80, 85, 90, 95, 100, 105, 110)
boxplot(points_scored, main = "Boxplot of Points Scored by Players",
        ylab = "Points Scored", col = "lightblue")
points(jitter(rep(1, length(points_scored))), points_scored, col = "red", pch = 19)
summary(points_scored)
